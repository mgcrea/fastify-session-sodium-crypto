{
  "name": "@mgcrea/fastify-session-sodium-crypto",
  "version": "0.8.7",
  "description": "Fast sodium-based crypto for fastify-session",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/fastify-session-sodium-crypto",
  "license": "MIT",
  "access": "public",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "fastify-session-sodium-crypto": "./dist/bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run spec -- --watch",
    "build": "tsup --entry src/index.ts --format cjs,esm --sourcemap --dts --clean --entry src/bin/cli.ts",
    "postbuild": "shebang-trim dist/bin/cli.js dist/bin/cli.cjs",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "prettify": "prettier --write src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "DEBUG=fastify-session* vitest --run",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fastify",
    "session",
    "sodium",
    "crypto"
  ],
  "dependencies": {
    "sodium-native": "^4.0.0"
  },
  "peerDependencies": {
    "@mgcrea/fastify-session": "^0.16.0"
  },
  "devDependencies": {
    "@mgcrea/fastify-session": "^0.16.0",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/node": "^18.11.18",
    "@types/sodium-native": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.2",
    "prettier-plugin-organize-imports": "^3.2.1",
    "shebang-trim": "^1.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.0"
  }
}
